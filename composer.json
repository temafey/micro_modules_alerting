{
    "name": "micro-module/alerting",
    "type": "library",
    "description": "Micro module Alerting common library",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-intl": "*",
        "ext-redis": "*",
        "beberlei/assert": "^3.3",
        "micro-module/base": "^0.7",
        "micro-module/value-object": "^0.6",
        "queue-interop/amqp-interop": "^0.8",
        "queue-interop/queue-interop": "^0.7|^0.8",
        "micro-module/enqueue": "^0.10",
        "micro-module/tactician-command-events": "^0.8",
        "psr/log": "^1.0",
        "php-http/guzzle7-adapter": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "micro-module/unit-test-generator": "^0.6",
        "mockery/mockery": "^1.3",
        "php-parallel-lint/php-console-highlighter": "^0.5",
        "php-parallel-lint/php-parallel-lint": "^1.0",
        "phan/phan": "4.0.*",
        "phpmd/phpmd": "^2.8",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-mockery": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-beberlei-assert": "^0.12",
        "phpunit/phpunit": "^9.4",
        "qossmic/deptrac-shim": "^0.11",
        "roave/security-advisories": "dev-master",
        "symplify/easy-coding-standard": "^9.0",
        "vimeo/psalm": "^4.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MicroModule\\Alerting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MicroModule\\Alerting\\Tests\\Unit\\": "tests/Unit"
        }
    },
    "minimum-stability": "beta"
}
